namespace gix_guard {
  u32 add(u32 a, u32 b);
};


[Error]
enum GixTunnelErrorKind {
  "Ok",
  "Failed",
  "NullInput",
  "InvalidInput",
  "InvalidInputLength"
};

interface PrivateKey {
    [Throws=GixTunnelErrorKind]
    constructor(sequence<u8> key);

    [Name=from_hex_key, Throws=GixTunnelErrorKind]
    constructor(sequence<u8> hex_key);

    [Name=from_base64_str, Throws=GixTunnelErrorKind]
    constructor(string base64_str);
};